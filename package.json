{
  "name": "opensumi-devtools",
  "version": "1.2.1",
  "type": "module",
  "description": "A Chrome DevTools Extension for OpenSumi.",
  "license": "MIT",
  "repository": "git@github.com:opensumi/devtools.git",
  "scripts": {
    "build": "cross-env NODE_ENV='production' BABEL_ENV='production' node utils/build.js",
    "start": "cross-env NODE_ENV='development' BABEL_ENV='development' node utils/server.js",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,yml}\"",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,yml}\"",
    "pretty-quick:check": "pretty-quick --staged --check --pattern \"**/*.{js,jsx,ts,tsx,json,css,scss,yml}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-data-grid": "^7.0.0-beta.43",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.0",
    "react-json-view": "^1.21.3",
    "react-tabs": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@types/chrome": "^0.0.177",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.8.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lodash-es": "^4.17.21",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "querystring": "^0.2.1",
    "sass": "^1.52.3",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^3.0.1",
    "ssestream": "1.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.3.1"
  }
}
